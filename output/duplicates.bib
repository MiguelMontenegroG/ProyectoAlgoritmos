@article{5386178,
 abstract = {Processes are everywhere. Organizations have business processes to manufacture products, provide services, purchase goods, handle applications, etc. Also in our daily lives we are involved in a variety of processes, for example when we use our car or when we book a trip via the Internet. Although such operational processes are omnipresent, they are at the same time intangible. Unlike a product or a piece of data, processes are less concrete because of their dynamic nature. However, more and more information about these processes is captured in the form of event logs. Contemporary systems ranging from copiers and medical devices to enterprise information systems and cloud infrastructures record events. These events can be used to make processes visible. Using process mining techniques it is possible to discover processes. This provides the insights necessary to manage, control, and improve processes. Process mining has been successfully applied in a variety of domains ranging from healthcare and e-business to high-tech systems and auditing. Despite these successes, there are still many challenges as process discovery shows that the real processes are more},
 author = {van der Aalst, W.M.P.},
 doi = {10.1109/MCI.2009.935307},
 journal = {IEEE Computational Intelligence Magazine},
 title = {Process Discovery: Capturing the Invisible},
 year = {2010}
}

@inproceedings{5393938,
 abstract = {This paper introduces a method for better communication between collaborating engineers and between engineer and entity generation processes at the definition of products in modeling systems. The authors recognized the importance of correct human-computer- human communication by using of product model as a special medium for engineers. In order to establish a product model that is appropriate for this purpose, they integrated all influencing effects on a product in an influencing space. In order to record content of influencing by engineers and other influencing persons, authorities, etc., the currently prevailing information based product model is completed by representation of information content. This paper emphasizes some relevant issues of information content. Definition of this contents starts from modeling of human intent. Intent model is consists of information about the owner of an intent, characteristics of that intent, and representation of content of intent in some form of knowledge. Information content is organized in the interconnected intent, behavior, and decision spaces. In a hierarchical system, intent space is at the highest and decision space is at the lowest level hierarchy assuring human intent based automation of decision making on engineering object in an improved product model.},
 author = {Horváth, László and Rudas, Imre J. and Hancke, Gerhard},
 booktitle = {2009 IEEE International Conference on Computational Cybernetics (ICCC)},
 doi = {10.1109/ICCCYB.2009.5393938},
 title = {On the way towards model for background of decisions on engineering objects in PLM},
 year = {2009}
}

