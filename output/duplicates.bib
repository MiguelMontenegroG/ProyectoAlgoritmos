@inproceedings{11147408,
 abstract = {This study introduces TechPark, an innovative educational tool designed to introduce middle school students to fundamental concepts in 3D modeling and computational geometry. TechPark is an interactive software tool in which children create their own 3D objects through various iterations, gaining insight into how 3D modeling works. The tool enhances spatial reasoning and deepens students’ understanding of geometry and mathematics by engaging them with real-time 3D object modeling, interactive quizzes, and AI-driven visualization tools. The software utilizes cutting-edge deep learning algorithms to transform real-world objects into 3D models, enabling students to explore key principles such as},
 author = {Marin, Andres and Movahed, Saniya Vahedian},
 booktitle = {2025 IEEE Integrated STEM Education Conference (ISEC)},
 doi = {10.1109/ISEC64801.2025.11147408},
 title = {TechPark: A Novel Approach for Middle Schoolers to Learn 3D Image Reconstruction from Stereo Images},
 year = {2025}
