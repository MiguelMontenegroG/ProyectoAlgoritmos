@inproceedings{5376385,
 abstract = {Role as new abstraction has been gradually used in agent analysis and design research field, and it is generally defined as an entity or a set of properties. The relationship between role and agent is relative independent, and role model is established before agent model. In this paper, we think role and agent is interdependent, agent is carrier of role, and role means what agent can do. When analysis and design agent model, should firstly have agent entity, then have role. We define role as a set of computation specification and function to realize relationship between two agents, establish a mapping between BDI model and role, and define an agent BDI model based on role (RA) using Pi calculus. Based on theory analysis, RA can keep balance among belief, desire and intention, and maintain model stable. In the end, we give an example of RA, example show RA can be conveniently established and used to construct agent goal solving organization.},
 author = {Zhang, Jian and Zeng, Guang-Zhou},
 booktitle = {2009 International Conference on Artificial Intelligence and Computational Intelligence},
 doi = {10.1109/AICI.2009.135},
 title = {RA-An Agent BDI Model Based on Role},
 year = {2009}
}

@inproceedings{5376537,
 abstract = {Sentence group is mapped into contextual element, one of the four layers of linguistic conceptual space in the theory of hierarchical network of concepts (HNC). The contextual element is composed of three parts: domain, situation and background. The domain is extracted by merging sentences with the same or similar domain conceptual symbol, which comes from the symbols of conceptual primitives, the conceptual symbols of words, and the semantic chunks in a sentence. The conceptual framework of situation is filled with the right words or phrases, according to the knowledge of domain sentence categories, the sentence categories and semantic chunks. The experiments show that there are still many aspects to be improved, including the knowledge base, analyzing the semantic structures of sentences, and matching the conceptual symbols in different types.},
 author = {Wei, Xiangfeng and Zhang, Quan},
 booktitle = {2009 International Conference on Artificial Intelligence and Computational Intelligence},
 doi = {10.1109/AICI.2009.474},
 title = {The Method of Extracting Domain and Situation in Chinese Sentence Groups},
