@article{11077161,
 abstract = {AI product incubation faces unique challenges such as difficulty in technical feasibility assessment, ambiguous value perception, and engineering transformation barriers. This research proposes the PRISE framework (Problem Definition, Rapid Validation, Impact Evaluation, Single-focus Development, End-to-End Productization), integrating design thinking, lean startup, and engineering practices with adaptive modifications for AI characteristics. Through analysis of seven cross-industry cases from Microsoft’s AI Vertical team, we validated the framework’s practicality. The research reveals five key patterns for successful AI product incubation: user-decision maker separation identification, lightweight validation priority, technology-value transformation mechanism, deep cultivation of vertical scenarios, and end-to-end delivery thinking. This framework fills the gap in AI product development methodology, providing systematic guidance and operational tools for practitioners while offering new perspectives for AI innovation management theory.},
 author = {Wang, Baoli and Li, Ye and Lei, Wen},
 doi = {10.1109/ACCESS.2025.3587943},
 journal = {IEEE Access},
 title = {PRISE: A Framework for AI Product Incubation From Concept to Implementation},
 year = {2025}
}

@inproceedings{11091859,
 abstract = {With the advancement of information technology, Computer-Supported Knowledge Building (CSKB) has become a crucial educational paradigm. Collaborative Problem Solving (CPS) is a key skill in CSKB, enhancing individual cognitive abilities and interpersonal communication skills. Existing research shows that students engaging in CPS often develop stronger critical thinking and creative problem solving skills. However, the specific mechanisms through which CPS impacts academic performance in CSKB contexts remain to be explored. This study focuses on 51 undergraduate students from various majors, based on a three-stage instructional model. By integrating the Knowledge Forum platform with the KBDeX analysis tool, this study aims to delve into the specific mechanisms by which computer-supported collaborative problem solving abilities impact student academic performance during the process of knowledge building. The findings reveal a significant positive correlation between the number of students’ personal notes and their academic performance, verifying the facilitative effect of knowledge externalization behaviors on the internalization of learning. Furthermore, this study uncovers the hierarchical mechanism of CPS abilities effects within CSKB environments, emphasizing the importance of phased instructional design, which provide theoretical evidence for optimizing computer-supported educational interventions and offer guidance for curriculum development, instructional strategies, and practical integration of technology in education.},
 author = {Huang, Yuting and Shao, Yuandi and Zhao, Xinyue and Ma, Qianqian and Yang, Yuqin},
 booktitle = {2025 7th International Conference on Computer Science and Technologies in Education (CSTE)},
 doi = {10.1109/CSTE64638.2025.11091859},
