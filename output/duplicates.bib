 abstract = {The IEEE ISIT 2024 “Bits and Bots Competition” was held in conjunction with ISIT 2024 in Athens, Greece. The competition challenged participants to detect and generate bots that simulate interaction with a website in real-world scenarios, bringing attention to the growing importance of automated agent detection in web security. Seventy participants, organized into more than 12 teams from institutions across the globe, took part in the event. This was the second edition of a data science-focused competition organized by the Student and Outreach Committee of the Information Theory Society, aiming to foster innovation and practical problem-solving skills among students. The competition culminated in four teams winning the final prize and presenting their solutions during ISIT 2024. This report highlights the student experience, as well as the diverse and creative approaches developed by the four winning teams.},
 author = {Rinis, Stefano and Shirani, Farhad},
 doi = {10.1109/MBITS.2025.3587093},
 journal = {IEEE BITS the Information Theory Magazine},
 title = {Event Report: IEEE ISIT 2024 “Bits and Bots” Competition – Fostering Innovation and Learning through Bot Detection Challenges},
 year = {2025}
}

@inproceedings{11075441,
 abstract = {The field of education in the digital era is undergoing tremendous changes, and computational thinking, as the basis for understanding and applying digital technologies, has become a key skill for learners in the 21st century. Challenges to the development of computational thinking in junior high school students include a single teaching method, teachers’ overemphasis on programming language learning, a lack of systematic teaching content, and students’ lack of problem-solving skills, which affect the development of computational thinking. Based on this, this study takes ninth-grade students as the research object, and adopts Quasi-experimental and Survey research methods under the guidance of constructivism theory, theory of mind and learning community theory to construct and validate a PBL teaching mode based on the cultivation of computational thinking in junior high school information technology curriculum. The results show that the PBL teaching mode for computational thinking cultivation is applied to the junior high school Mixly course to improve the computational thinking level of students. This study provides theoretical reference and practical significance for Mixly teaching oriented towards CT cultivation.},
