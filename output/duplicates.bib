@inproceedings{10838733,
 abstract = {While Agent-Based Models can create detailed artificial societies based on individual differences and local context, they can be computationally intensive. Modelers may offset these costs through a parsimonious use of the model, for example by using smaller population sizes (which limits analyses in sub-populations), running fewer what-if scenarios, or accepting more uncertainty by performing fewer simulations. Alternatively, researchers may accelerate simulations via hardware solutions (e.g., GPU parallelism) or approximation approaches that operate a tradeoff between accuracy and compute time. In this paper, we present an approximation that combines agents who ‘think alike’, thus reducing the population size and the compute time. Our innovation relies on representing agent behaviors as networks of rules (Fuzzy Cognitive Maps) and empirically evaluating different measures of distance between these networks. Then, we form groups of think-alike agents via community detection and simplify them to a representative agent. Case studies show that our simplifications remain accuracy.},
 author = {Giabbanelli, Philippe J. and Beerman, Jack T.},
 booktitle = {2024 Winter Simulation Conference (WSC)},
 doi = {10.1109/WSC63780.2024.10838733},
 title = {Accelerating Hybrid Agent-Based Models and Fuzzy Cognitive Maps: How to Combine Agents who Think Alike?},
 year = {2024}
}

@inproceedings{10840805,
 abstract = {In this study., we present a feature prioritization framework based on a User-Centered Design (UCD) approach., aimed at optimizing the selection of product features from both user and business perspectives. Using a weighted scoring system, we evaluate features across three main criteria: User Impact, Business Value, and Technical Feasibility. The results provide a clear ranking of features based on their overall total score, enabling product teams to make data-driven decisions on which features to prioritize in the development lifecycle. The framework balances user needs with business goals, ensuring that the chosen features deliver maximum value while maintaining technical feasibility. This methodology supports agile decision-making and enhances the alignment between user experience and business outcomes.},
 author = {Krishan, Chitra and Choubey, Amrish Kumar and Thakur, Ayush and Gupta, Astha and Verma, Pranay},
 booktitle = {2024 4th International Conference on Technological Advancements in Computational Sciences (ICTACS)},
 doi = {10.1109/ICTACS62700.2024.10840805},
 title = {Enhancing Product Management Through User-Centric Design: A Human-Computer Interaction Perspective},
 year = {2024}
}

@inproceedings{10840839,
 abstract = {Generative adversarial networks (GANs) have achieved remarkable success in image synthesis, but evaluating their quality remains an open challenge. Existing automated metrics like FID are unable to fully capture generation performance across both objective fidelity and subjective aesthetic attributes important to human perception. This paper conducts an extensive review of the literature on GAN evaluation methodology. Key limitations of current approaches are analyzed through a discussion of popular metrics and their inability to predict human judgment comprehensively. To address this, a novel hybrid evaluation model is proposed that combines automated scores with human ratings to provide a more balanced assessment. A pilot study demonstrates the hybrid metric can predict human response more accurately.},
 author = {Sukhani, Gautam and Gulhane, Monali and Rakesh, Nitin and Maurya, Sudhanshu and Choudhary, Shilpa and Pandey, Senam},
 booktitle = {2024 4th International Conference on Technological Advancements in Computational Sciences (ICTACS)},
 doi = {10.1109/ICTACS62700.2024.10840839},
 title = {Leveraging Machine Learning Techniques for Predictive Maintenance and Fault Detection in Industrial Systems},
 year = {2024}
}

@inproceedings{10844653,
 abstract = {This feasibility study investigates the practicality of incorporating Computational Thinking (CT) education into secondary school curricula, aiming to equip students with essential 21st-century skills. The study assesses the readiness of educational institutions, teachers and students to embrace CT as an integral part of their learning journey, identifying potential challenges and barriers to successful implementation. To achieve these objectives, the research examines the existing landscape of CT education, evaluates teacher preparedness, analyses student receptiveness, and explores resource requirements and best practices from around the world. As part of this investigation, an immersive learning approach will be integrated. A five-day training program was implemented, featuring a comprehensive curriculum that covered key CT concepts, problem-solving techniques, presentation skills, and problem-based learning aligned with Sustainable Development Goals. The program included pre-and post-surveys to measure students’ understanding of CT concepts and practical projects to apply their learning. Results showed consistent improvement in students’ self-reported understanding of CT skills and their ability to apply these concepts to real-world problems. The study reveals that integrating CT into secondary education is not only necessary but also highly beneficial. The study emphasizes that a holistic educational reform incorporating leadership, technology and curriculum adjustments is essential for the successful implementation of CT education. The study also highlights the importance of aligning CT education with current educational policies and trends, such as KSSM (Kurikulum Standard Sekolah Menengah -Secondary School Standard Curricular), which advocate for the inclusion of CT as a fundamental practice of science education. In conclusion, this study provides valuable insights and recommendations for educational policymakers, administrators, and educators interested in enhancing CT skills in secondary students.},
 author = {Ghazali, Khadijah Wan Mohd and Osman, Mashanum and Othman, Zuraini and Omar, Masitah Seikh},
 booktitle = {2024 International Conference on TVET Excellence & Development (ICTeD)},
 doi = {10.1109/ICTeD62334.2024.10844653},
 title = {Integrating Computational Thinking Skills in Secondary Education: a Feasibility Study},
 year = {2024}
}

@inproceedings{10847381,
 abstract = {The use of information and communication technology has evolved tremendously in the last decade. The dependency on this technology is an integral part of any business operations to achieve their strategic goal, which means compromise to any of these components, can severely disrupt and jeopardise business activities and can also bring a bad name to their reputation. As technology is becoming more sophisticated, there is a rise in more organised cyber-attacks. Organisations struggle to develop an effective Cyber security strategy, mainly because more user awareness is required in the domain. Since the number of Internet users and the integration of technologies in our lives is increasing exponentially. Humans live in a cyber social system, in which they interact with smart infrastructure. Attackers exploit humans as the weakest link to execute successful security breaches and gain an advantage in the lucrative cybercrime market. There is a dire need to address new challenges posed by cybercriminals and mitigate risks to human digital lives.},
