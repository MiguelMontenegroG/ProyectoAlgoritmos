@inproceedings{10755621,
 abstract = {The development of three-dimensional geometry problem-solving skills is important for engineering students and professionals. Acquiring these skills through traditional methods often presents challenges for students, mainly because the teaching material is presented in plain images with which students cannot interact to gain greater understanding. This paper describes the development of a computational tool designed to transform plain axonometric projection images into 3D models compatible with CAD design software. To achieve this, we have engineered deep learning models to accurately classify, locate, and identify geometric elements within axonometric images. Our experimental results highlight the enhanced performance of the Faster-RCNN model, which achieved an 81 % mean Average Precision (mAP) on test images. This tool can help engineering students to have a better learning experience, allowing them to interact with three-dimensional representations and significantly improve their spatial understanding.},
 author = {Apaza Condori, Juan Pablo and Villanueva, Edwin},
 booktitle = {2024 IEEE ANDESCON},
 doi = {10.1109/ANDESCON61840.2024.10755621},
 title = {Detection of Geometric Elements in Axonometric Projection Images Using Computer Vision Techniques},
 year = {2024}
}

@inproceedings{10758531,
 abstract = {The article is devoted to the problem of formalization and modeling of human reasoning for their further automation and software implementation. The purpose of such automation can be the development of intellectual assistants in various spheres of activity. The intellectual assistant in prospect can partially or completely replace the sales manager, conducting argumentative dialog with the client in automatic or semi-automatic mode. Also, as an application of methods for automating human reasoning can be the development of a digital consultant, which selects for the manager the possible options of proposals that can satisfy the customer. The manager then selects the most suitable options from his point of view, offers them to the customer and explains why these options will suit the customer. We continue to develop a model-theoretic approach to the formalization of thinking, reasoning, consciousness, and reflection based on the application of the Partial Model Theory. In this paper, we formalize and model purposeful reasoning aimed at solving the problem of achieving the “ideal final result” required by the client. For this purpose, we construct a sequence of partial models, each of which is a supermodel of the previous one, specifying the client},
 author = {Palchunov, Dmitry E.},
 booktitle = {2024 IEEE International Multi-Conference on Engineering, Computer and Information Sciences (SIBIRCON)},
 doi = {10.1109/SIBIRCON63777.2024.10758531},
 title = {Logical Methods for Modeling Reasoning, Concepts and Representations Based on the Partial Model Theory},
 year = {2024}
}

@inproceedings{10761137,
 abstract = {In recent years, large-scale text-driven image synthesis models have garnered widespread attention for their ability to generate highly diverse images from textual prompts. Techniques like DALL.E2 and CLIP have shown significant success in producing high-quality images from descriptions. However, the challenge of achieving precise and creative image editing while preserving the original content persists. This study proposes a novel image editing method based on the Prompt-to-Prompt approach combined with large language models. The method aims to provide an intuitive and efficient editing experience, allowing users to perform detailed modifications without needing professional expertise in image processing. By leveraging advanced diffusion models, this approach enhances the flexibility of text-driven image editing, making it more accessible to a broader audience.},
 author = {Wu, Xinyu},
 booktitle = {2024 IEEE 7th International Conference on Information Systems and Computer Aided Education (ICISCAE)},
 doi = {10.1109/ICISCAE62304.2024.10761137},
 title = {Text-Driven Image Editing Based on Diffusion Models},
 year = {2024}
}

@inproceedings{10762190,
 abstract = {Advanced mathematics is a fundamental subject in college education, essential for developing students’ analytical and problem-solving skills. It forms the basis for many advanced topics and is crucial for a well-rounded academic foundation. Mathematical modeling, on the other hand, is a powerful tool for addressing real-world problems by creating abstract models. It is widely applied in fields like economics, industry, medicine, and social sciences. With the rapid advancement of computer technology, mathematical modeling has evolved significantly. Tools like MATLAB have become essential for developing and analyzing complex models, enabling the resolution of intricate problems that are difficult to solve manually. The integration of computer technology into mathematical modeling enhances both the efficiency and accuracy of problem-solving.Given these advancements, it is crucial for colleges and universities to incorporate computer-based mathematical modeling into their curriculum. By integrating these techniques into advanced mathematics courses, educators can create a more cohesive learning experience that combines theoretical knowledge with practical applications. This approach not only improves the quality of mathematics education but also better prepares students to apply their mathematical skills to real-world challenges, equipping them for professional success in a rapidly changing world.},
 author = {Hu, Yangchun and Shi, Shaojun},
 booktitle = {2024 5th International Conference on Big Data & Artificial Intelligence & Software Engineering (ICBASE)},
 doi = {10.1109/ICBASE63199.2024.10762190},
 title = {Application and Fusion Analysis of Advanced Mathematical Modeling Thought Based on Computer Technology in University Teaching},
 year = {2024}
}

@article{10766404,
 abstract = {Unmanned aerial vehicle (UAV)-assisted mobile edge computing (MEC) is emerging as a promising paradigm to provide aerial-terrestrial computing services in close proximity to mobile devices (MDs). However, meeting the demands of computation-intensive and delay-sensitive tasks for MDs poses several challenges, including the demand-supply contradiction between MDs and MEC servers, the demand-supply discrepancy between MDs and MEC servers, the trajectory control requirements on energy efficiency and timeliness, and the different time-scale dynamics of the network. To address these issues, we first present a hierarchical architecture by incorporating terrestrial-aerial computing capabilities and leveraging UAV flexibility. Furthermore, we formulate a joint computing resource allocation, computation offloading, and trajectory control problem to maximize the system utility. Since the problem is a non-convex and NP-hard mixed integer nonlinear programming (MINLP), we propose a two-timescale joint computing resource allocation, computation offloading, and trajectory control (TJCCT) approach for solving the problem. In the short timescale, we propose a price-incentive model for on-demand computing resource allocation and a matching mechanism-based method for computation offloading. In the long timescale, we propose a convex optimization-based method for UAV trajectory control. Besides, we theoretically prove the stability and polynomial complexity of TJCCT. Extensive simulation results demonstrate that the proposed TJCCT is able to achieve superior performances in terms of the system utility, average processing rate, average completion delay, average completion ratio, and average cost, while meeting the energy constraints despite the trade-off of the increased energy consumption.},
 author = {Sun, Zemin and Sun, Geng and Wu, Qingqing and He, Long and Liang, Shuang and Pan, Hongyang and Niyato, Dusit and Yuen, Chau and Leung, Victor C. M.},
 doi = {10.1109/TMC.2024.3505155},
 journal = {IEEE Transactions on Mobile Computing},
 title = {TJCCT: A Two-Timescale Approach for UAV-Assisted Mobile Edge Computing},
 year = {2025}
}

