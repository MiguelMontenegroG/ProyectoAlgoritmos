 title = {IEML-A New Frontier for Semantic Computing and Collective Intelligence in the Cooking Domain},
 year = {2025}
}

@inproceedings{11108534,
 abstract = {Large language models(LLMs) demonstrate exceptional performance in understanding and generating language but face challenges in tackling complex decision-making tasks. Researchers have introduced LLM-based agent workflows to address real-world complex problems. However, these workflows rely heavily on manual design, which is not only time-intensive but also requires significant expertise and lacks adaptability to diverse scenarios and dynamic environments. This paper introduces a novel approach for automatic generation of agentic workflows powered by LLMs, leveraging a meta-cognitive based Chain-of-Thought (MC-CoT) guidance integrated with Monte Carlo Tree Search (MCTS). By combining MC-CoT for structured reasoning with MCTS for optimization, the framework dynamically adjusts workflows, delivering superior performance across benchmarks such as GSM8K and MATH. Experimental results demonstrate the system},
 author = {Zhou, Kaiwen and Du, Wenli and Tian, Xufei},
 booktitle = {2025 7th International Conference on Natural Language Processing (ICNLP)},
 doi = {10.1109/ICNLP65360.2025.11108534},
 title = {Agentic Workflows Generation Based on Meta-Cognitive Chain-of-Thought Guided Monte Carlo Tree Search},
 year = {2025}
}

@inproceedings{11113568,
 abstract = {Basic Programming Practice (BPP), as an introductory course for computer science majors, aims to enable students to have basic programming skills and lay a foundation for subsequent advanced course learning. This article draws on the concept of “using qi and cultivating both inside and outside” in traditional Chinese martial arts and highlights the importance of “thinking style” as an internal factor, developing a Three-stage Thinking (TT) methodology to foster students},
 author = {Cai, Xiaojun and Keung, Jacky and Shen, Zhaoyan and Zhao, Mengying and Yang, Zhen and Jia, Zhenge and Han, Fangxi and Yu, Dongxiao},
 booktitle = {2025 International Symposium on Educational Technology (ISET)},
 doi = {10.1109/ISET65607.2025.00048},
 title = {Improving Learning Efficacy in Basic Programming Practice Based on Three-Stage Thinking},
 year = {2025}
}

@inproceedings{11120412,
 abstract = {This study aims to guide pre-service teachers in effectively utilizing Generative Artificial Intelligence (GAI) tools to enhance their Artificial Intelligence (AI) literacy. By ana-lyzing the impact of GAI on the AI literacy of pre-service teachers, this study proposes a “teacher-student-machine” triadic interactive teaching model based on self-organized learning and deep empowerment through GAI. Using an experimental class in a software engineering course as a case study, we construct an end-to-end experimental environment for the project-driven teaching lifecycle, covering requirement analysis, system design, development and implementation, as well as software testing and maintenance phases. GAI is used to support the completion of experimental tasks while fostering pre-service teachers},
 author = {Zhang, Junna and Sun, Yulin and Duan, Li and Liu, Hui and Liu, Chunhong and Zhang, Aili and Yuan, Peiyan},
 booktitle = {2025 IEEE International Conference on Software Services Engineering (SSE)},
 doi = {10.1109/SSE67621.2025.00034},
 title = {A Practical Teaching Model of Software Engineering Courses for Artificial Intelligence Literacy Cultivation in Pre-Service Teachers},
 year = {2025}
}

@article{11126173,
 abstract = {High-power microwaves (HPMs) are short-pulse electromagnetic waves in the microwave spectrum, characterized by intense impact energy that can cause damage to both electronic devices and human organs. In recent years, with the rapid advancement of data-driven artificial intelligence paradigms, achieving intelligent protection against HPM has emerged as a critical research challenge. The key to enabling intelligent analysis of plasma-based HPM protection lies in accurately modeling the complex multiphysics interactions between HPM and plasma. Existing methods have made progress in predicting the response of plasma characteristic parameters (such as electron density and electric intensity) under HPM exposure and in imputing missing values. However, it primarily focuses on short-term variations of single-feature variables. In this work, we propose a novel time-series modeling approach that integrates multiperspective feature extraction with multihead attention-based information fusion, complemented by an autonomous thinking strategy (ATS). This framework enhances the precise capture of long-term multivariate time-series patterns, and our experiments demonstrate its effectiveness. Furthermore, this study specifically investigates the coupling effects between HPM and a cylindrical plasma array (CPA). By conducting case studies and comparing our method with three state-of-the-art time-series models, we validate the superior accuracy and generalizability of our proposed approach.},
 author = {Feng, Naixing and Dong, Chunzhi and Fu, Jianyuan and Zheng, Fanghua and Huang, Zhixiang},
 doi = {10.1109/TMTT.2025.3596862},
 journal = {IEEE Transactions on Microwave Theory and Techniques},
 title = {MVPTransformer: Long-Term Multivariate Time-Series Pattern for Plasma Array-Based Intelligent Protection Against HPM},
 year = {2025}
}

@inproceedings{11126322,
 abstract = {Cooperative embodied AI systems, where multiple agents collaborate to accomplish complex, long-horizon tasks, show significant promise for real-world applications. These systems integrate perception, cognition, and action through integrated task and motion planning (TAMP), leveraging the advanced reasoning and communication capabilities of large language models (LLMs). However, their efficiency is often hindered by challenges such as high computational latency and redundant communication, largely due to the reliance on LLMs for sequential planning decisions. In this paper, we aim to identify the inherent characteristics and optimization opportunities in cooperative embodied AI systems. We first present a cognitive-inspired modular framework encompassing perception, memory, communication, planning, and execution. We then conduct a detailed profiling analysis of two state-of-the-art cooperative embodied systems, revealing the significance of each module and identifying critical bottlenecks such as redundant message pre-generation and excessive LLM usage in decision-making processes. Based on these insights, we propose several model- and system-level optimizations, including a planning-first communication strategy, selective multi-agent communication, and planning-guided multi-step execution. Evaluated across long-horizon cooperative tasks, these optimizations reduce the frequency of LLM inference runs, achieving an average 3.93× speedup in end-to-end task execution. Finally, we discuss the challenges and potential directions for embodied AI computing, to enhance system flexibility, efficiency, and scalability.},
 author = {Wan, Zishen and Du, Yuhang and Ibrahim, Mohamed and Zhao, Yang and Krishna, Tushar and Raychowdhury, Arijit},
 booktitle = {2024 ACM/IEEE International Conference On Computer Aided Design (ICCAD)},
 doi = {10.1145/3676536.3698389},
 title = {Thinking and Moving: An Efficient Computing Approach for Integrated Task and Motion Planning in Cooperative Embodied AI Systems (Invited Paper)},
 year = {2024}
}

@inproceedings{11129395,
 abstract = {This study aimed to develop a STEAM-based instructional activity package to enhance learning in the Computer-Based Control Systems subject and support 21stcentury competencies. The package included knowledge sheets, pre- and post-tests, achievement assessments, instructional media, worksheets, and IoT kits. Instrument validation was conducted by five experts, with analyses of item difficulty, discrimination, and reliability. The sample comprised 30 thirdyear students in the Industrial Electrical Engineering (IEE) program at Nakhon Sawan Rajabhat University. Results showed a significant improvement in learning outcomes, with pre-test and post-test averages of 26.50 and 45.67, respectively ($\mathbf{p},
 author = {Sonthitham, Pischanunt and Yodnil, Krisana and Sonthitham, Arkira},
 booktitle = {2025 10th International STEM Education Conference (iSTEM-Ed)},
 doi = {10.1109/iSTEM-Ed65612.2025.11129395},
 title = {Development of Instructional Activity Package Based on STEAM Education to Enhance Learning in Computer-Based Control Systems Subject},
 year = {2025}
}

@inproceedings{11129525,
 abstract = {In the realm of creative computing, the transition from divergent to convergent thinking has lacked a systematic approach, hindering the integration of these cognitive processes. This study introduces a method designed to address this issue. Two primary types of thinking are examined: “Imagining” under divergent thinking and “Inference” under convergent thinking. The approach relies on a crucial transitional mechanism emotional assessment. By evaluating the emotional impact of ideas generated during the “Imagining” phase, those evoking positive emotional reactions are prioritised and linked to the “Inference” phase, where these ideas are refined into practical applications. This strategic use of emotional evaluations as a bridge between divergent and convergent thinking phases offers a structured pathway that enhances both the generation and implementation of creative ideas in computing, representing a theoretical and practical advancement in the field.},
 author = {Zeng, Chong and Yang, Hongji},
 booktitle = {2025 IEEE 11th International Conference on Big Data Computing Service and Machine Learning Applications (BigDataService)},
 doi = {10.1109/BigDataService65758.2025.00008},
 title = {An Approach to Transiting from Divergent Thinking to Convergent Thinking for Creativity},
 year = {2025}
}

@inproceedings{11131953,
 abstract = {Classroom Escape”, an activity that was part of the “Hour of Code” project, was conducted with 268 upper-grade students from four elementary schools during an IT class. The topic of the activity was marine flora and fauna, but the teaching itself belonged to the domain of Computational Thinking and Programming. Through four tasks, student teams, using a project-based approach and collaboration, had to uncover questions, answer them, and progress to the next level. Artificial intelligence was used in the “Teachable Machine” application on a connected computer. The activity utilised the Scratch Lab application, a Caesar cipher cylinder, Morse code, the ASCII table and computers with Internet access. Relatively complex content was taught using innovative methods-namely, a projectbased, multidisciplinary approach-making the lessons engaging, interesting, and appealing. Students are eager to learn through a project-based approach and gamification, even though they often rely on trial and error and sometimes struggle to find the solution. Educational games like “Classroom Escape” demonstrate an effective way to enhance the teaching and learning of programming concepts. A game-based approach can help engage students, improve their problem-solving skills, and develop their computational thinking abilities.},
 author = {Žufić, J. and Žufić, A. and Bodiš, S.},
 booktitle = {2025 MIPRO 48th ICT and Electronics Convention},
 doi = {10.1109/MIPRO65660.2025.11131953},
 title = {Hour of Code: Classroom Escape},
 year = {2025}
}

@inproceedings{11132019,
 abstract = {This study explores the perspectives of prospective kindergarten teachers regarding the integration of computational thinking (CT) in early childhood education (ECE), focusing on their self-assessed CT skills and attitudes toward implementing CT-related activities in the ECE settings. Recognizing the growing importance of CT as a fundamental life skill, this research aimed to assess the readiness of university students enrolled in the ECE program to integrate CT into their professional practice. Students in the first, second and third year of the bachelor study program were asked to self-assess their CT knowledge, skills and attitudes in an online survey. The results indicate that students exhibit a varied level of CT knowledge and skills: they excel in creativity, whereas the algorithmic thinking skill is perceived to be the least developed skill. In addition, they expressed overall positive attitudes toward incorporating CT activities in ECE, highlighting the potential benefits for children},
 author = {Moguš, A. Mirković and Varga, R. and Kiralj, V.},
 booktitle = {2025 MIPRO 48th ICT and Electronics Convention},
 doi = {10.1109/MIPRO65660.2025.11132019},
 title = {Prospective Kindergarten Teachers},
 year = {2025}
}

@article{11134640,
 abstract = {Cognitive abilities of learners are predicted to be impacted by their use of artificial intelligence (AI) tools, although it is premature to determine long-term effects. However, learners’ perceptions remain relevant. The purpose of this study is to assess how learners perceive AI usage impacted their cognitive abilities.},
 author = {Krsmanovic, Gala and Deek, Rebecca A. and Deek, Fadi P.},
 doi = {10.1109/MC.2025.3576936},
 journal = {Computer},
 title = {The Development and Maintenance of Thinking Processes and Learning Abilities in the Era of Artificial Intelligence},
 year = {2025}
}

@inproceedings{11136693,
 abstract = {The design of a robust least squares extended algorithm for social network analysis proposes an innovative method that integrates robust statistics and graph structure modeling, aiming to solve the problems of existing least squares algorithms being sensitive to noise and unable to effectively handle abnormal connections that are common in social networks. By constructing a robust objective function based on the Huber loss function, introducing a graph Laplace regularization term to maintain the network topology, and designing an adaptive weight adjustment mechanism to dynamically correct the influence of abnormal nodes, combined with a random coordinate descent algorithm, an efficient solution for large-scale networks is achieved. Experiments on real data sets such as Twitter and Facebook show that the algorithm has an average absolute error of only 0.32 in a 10% noise injection scenario, an abnormal connection identification accuracy of 93.6%, and an average processing time of only 7.27 minutes for a single machine to process a million-node network, which verifies its practicality and robustness in tasks such as social network relationship prediction and community discovery, and provides a new theoretical tool for complex social data analysis.},
 author = {Li, Bentu},
 booktitle = {2025 International Conference on Intelligent Computing and Knowledge Extraction (ICICKE)},
 doi = {10.1109/ICICKE65317.2025.11136693},
 title = {Design of a Robust Least Squares Extended Algorithm for Social Network Analysis},
 year = {2025}
}

@inproceedings{11137606,
 abstract = {This study aims to compare the psychometric properties of mathematics problem-solving assessment tools generated by Generative AI (Chat GPT) and those developed by humans. The participants included 130 students from 4 schools, selected through stratified sampling based on school size, out of 84 schools under the secondary educational service area office Khon Kaen. The mathematical problem-solving assessments (AI-generated and human-developed) were administered via Google Forms. The study employed Construct Modeling and Unidimensional Rasch Analysis to evaluate validity evidence, reliability evidence, quality of item fit, and the Turing test. The findings indicate that both tools are similar in content validity, but the AI-generated assessment demonstrates slightly higher accuracy in several aspects. Nevertheless, the human-developed assessment offers a better balance between the test difficulty and the examinees},
 author = {Phansrisuk, Panjit and Junpeng, Putcharee and Intharah, Thanapong},
 booktitle = {2025 International Technical Conference on Circuits/Systems, Computers, and Communications (ITC-CSCC)},
 doi = {10.1109/ITC-CSCC66376.2025.11137606},
 title = {Comparison of the Psychometric Properties of Mathematical Problem-Solving Assessment Tools for Grade 7 Students: AI-Generated vs. Human-Developed},
 year = {2025}
}

@inproceedings{11137621,
 abstract = {This study compares an AI-generated English Reading Comprehension test (Claude) with a human-created version on the topic of places. The research aims to evaluate the quality of AI-generated assessments with human design. Data from 128 ninth-grade students were analyzed using the Rasch model. Findings indicate that the AI-generated test demonstrated comparable content validity (CVI ratings consistently 3–4) to the human-made version while exhibiting superior reliability within acceptable psychometric limits. In a Turing Test, two English proficiency experts were more successful in identifying AI-generated items compared to experts from other fields. These results support the potential of AI in developing valid and extensible educational assessments.},
 author = {Khamrassamee, Pimvaree and Junpeng, Putcharee and Intharah, Thanapong},
 booktitle = {2025 International Technical Conference on Circuits/Systems, Computers, and Communications (ITC-CSCC)},
 doi = {10.1109/ITC-CSCC66376.2025.11137621},
 title = {The Effectiveness of Using Large Language Model to Generate English Reading Comprehension Diagnostic Test: Generative AI and Human-Made comparison},
 year = {2025}
}

@inproceedings{11139209,
 abstract = {Computational Thinking (CT) promotes the systematic resolution of complex interdisciplinary problems through abstraction, decomposition, pattern recognition, and modularization. As an extension of scientific reasoning, CT enhances problem modeling, resource allocation, and decision-making efficiency across various domains. This paper examines the fundamental principles of CT and demonstrates its practical applications in Object-Oriented Programming (OOP) and the E-CARGO model. Furthermore, it presents a case study on immigration policy making, illustrating how abstraction and classification improve problem modeling, while divide-and-conquer techniques enable computation and task assignment. The experimental results validate the superiority of CT-driven methods over traditional approaches in speed, accuracy, and cost-effectiveness. By integrating human reasoning with computational methodologies, CT provides a fundamental methodology for addressing large-scale optimization challenges, advancing both scientific research and applied problem-solving.},
 author = {Gong, Ke and Zhu, Haibin and Yang, Tianshuo},
 booktitle = {2025 1st International Symposium on E-CARGO and Applications (E-CARGO)},
 doi = {10.1109/E-CARGO65996.2025.11139209},
 title = {Computational Thinking, Object-Oriented Programming to Role-Based Collaboration},
 year = {2025}
}

@inproceedings{11139353,
 abstract = {Social media platforms such as YouTube, TikTok, and Reddit generate vast amounts of user-generated content, making sentiment analysis essential for understanding public opinion. Traditional sentiment analysis methods often struggle with sarcasm, informal language, and evolving trends, leading to the rise of Large Language Models (LLMs) as more effective alternatives. However, the choice between cloud-based LLMs (e.g., ChatGPT-4o, Gemini 2.0 Flash) and local LLMs (e.g., Meta LLaMA 3.2, DeepSeek-R1 7b) remains an open question, with each approach offering distinct advantages and limitations.This study compares the performance of online and local LLMs in sentiment analysis, focusing on discussions about AI-generated art and cancel culture. Through structured sentiment classification and comparative evaluation, we assess each model’s accuracy, efficiency, and interpretative depth. Our findings indicate that cloud-based models excel in contextual comprehension, while local models provide enhanced privacy and structured data processing. The results highlight the trade-offs between computational power, data security, and adaptability, reinforcing the potential of hybrid approaches that integrate both cloud and local LLM capabilities for more effective sentiment analysis.The goal of this research is to provide a clear evaluation of different LLMs in sentiment analysis, offering practical insights into their strengths and weaknesses. By comparing models from multiple developers, we aim to determine the most effective deployment strategies for real-world applications, ensuring accurate and scalable sentiment evaluation in diverse online discussions.},
 author = {Suindikovna, Riza Akhitova and David Gennadievych, Novikov and Sadyrbekkyzy, Akbergen Dinara},
 booktitle = {2025 IEEE 5th International Conference on Smart Information Systems and Technologies (SIST)},
 doi = {10.1109/SIST61657.2025.11139353},
 title = {From Comments to Insights: Comparing Local and Online LLM Models in Social Media Sentiment Evaluation},
 year = {2025}
}

@inproceedings{11141205,
 abstract = {Our analysis introduces how visual metaphors are used as a design tool in hybrid product design and exploratory, speculative use of the Contrastive LanguageImage Pretraining (CLIP) model. As hybrid products bridge the gap between physical and digital, then the use of AI generated visuals as metaphors can entice a more seamless transition for the user. Using an experimental method, the research analyses the kinds of visual metaphors and their influence on user satisfaction, commitment, and the perceived ability of the product. The findings indicate CLIP-inspired metaphors are highly capable of establishing an emotional connection getting a score of 9.5 out of 10, while higher concept designs perform better with more literal, intuitively aligned metaphors. This paper has demonstrated the potential of this AI approach to optimize the design process, resulting in personalized and context-appropriate metaphors that enhance user interaction and satisfaction. The insights gathered from the research foster a better understanding of the potential of AI in hybrid product development and the users that can be developed from it.},
 author = {Mohammed, Sumayah Abd Al Whaab and Lelo, Ahmed Talib},
 booktitle = {2025 IEEE 4th International Conference on Computing and Machine Intelligence (ICMI)},
 doi = {10.1109/ICMI65310.2025.11141205},
 title = {The Role of Visual Metaphors in Hybrid Product Design Using AI},
 year = {2025}
}

@inproceedings{11141259,
 abstract = {Nature has long been an inspiration for the design of various industrial products, boasting optimized structures and functional efficiency. The aim in this study is to help apply the new generation technologies, to take a source of inspiration from nature for new generation industrial designs. Mathematical techniques like fractals are being used to model organic systems accurately to improve product efficiency, beauty, and sustainability. The study assesses biomimetic designs using three primary metrics: fractal similarity, structural efficiency, and human expert opinion. The findings show that biomimetic designs are superior both structurally and aesthetically than traditional methods indicating a viable realistic application in verticals inclusive of architecture, automotive, and consumer electronics. These findings shed light on the transformative role in industrial design, merging the harmony of nature and technology.},
 author = {Sadiq, Safa Jaafar and Manea, Anis Hatem},
 booktitle = {2025 IEEE 4th International Conference on Computing and Machine Intelligence (ICMI)},
 doi = {10.1109/ICMI65310.2025.11141259},
 title = {Employing Modern Technologies in Drawing Inspiration from Nature to Develop Form in Industrial Products},
 year = {2025}
}

@inproceedings{11141300,
 abstract = {This paper investigates the changing role of Artificial Intelligence in terms of directing theatre productions and responding to the developing new territory of digital performances. To mitigate these hurdles of narrative coherence, aggregate interpretation of visual metaphor, and audience engagement, this study introduces a computational framework that embeds artificial intelligence in script analysis, scene management, and performance design. The methodology sutured together script-visual alignment models, emotional tone extractors, and staging optimizers in their own methodology, then tested it all on a data set of contemporary digital theater productions. The outcomes demonstrate considerable advancements in scene transitions, as well as director satisfaction and an audience emotional response, underscoring the ability for AI to be a co-creative partner rather than a replacement. These insights lay a robust foundation for the intelligent augmentation of both live and virtual performances, representing a paradigm shift in theatrical production practices.},
 author = {Latif, Ali Saad and Ismael, Muthanna Fadel and Khazal, Khattab Khaldon},
 booktitle = {2025 IEEE 4th International Conference on Computing and Machine Intelligence (ICMI)},
 doi = {10.1109/ICMI65310.2025.11141300},
 title = {The Role of Ai in Theater Directing: New Approaches to Digital Performances},
 year = {2025}
}

@article{11142324,
 abstract = {The integration of artificial intelligence into landscape architecture has opened new frontiers for computational design, enabling the generation of garden and outdoor spatial layouts that blend ecological logic with aesthetic innovation. This study presents a novel AI-driven framework tailored for garden design, synergizing symbolic formalism with neural generative modeling to support the synthesis of complex, context-aware landscape artifacts. Traditional garden design often relies on fixed templates or heuristic rules that limit adaptability to environmental constraints and stylistic variations. These methods struggle with semantic coherence and lack responsiveness to evolving cultural and ecological aesthetics. To overcome these challenges, we propose a dual-component system comprising StyleMorphNet and the Semantic Stylization Protocol (SSP). StyleMorphNet is a multi-branch neural architecture that decomposes the generative process into latent subspaces covering spatial structure, material composition, and stylistic innovation. This modular design supports fine-grained control over layout generation while preserving landscape semantics. Complementing it, SSP embeds domain-specific knowledge—including seasonal dynamics, functional zoning, and regional styles—into the generative loop via rule-conditioned modulation and symbolic-visual reasoning. Empirical evaluations show that our framework produces garden designs with superior spatial harmony, stylistic diversity, and ecological relevance compared to existing models. This work contributes to the field of computational landscape design by providing an adaptive and semantically grounded framework that supports the co-creation of expressive, sustainable outdoor environments.},
 author = {Yang, Han and Zheng, Dong and Wang, Lianyu},
 doi = {10.1109/ACCESS.2025.3602874},
 journal = {IEEE Access},
 title = {Deep Learning-Driven Garden Design: Integrating AI Into the Creative Process},
 year = {2025}
}

@inproceedings{11144581,
 abstract = {Intelligent machines supported by large language models and multimodal fusion technologies possess capabilities such as language understanding, deep learning, and autonomous cognition. They can simulate natural human-to-human dialogue interactions and promote human-machine collaborative decision-making that combines the advantages of human intelligence and machine intelligence. This article mainly explores the basic principles, key technologies, application scenarios, and future developments of the human-machine interactive collaborative decision-making system based on large model technology. Human-machine interactive collaborative decision-making unfolds the decision-making process through a chain of determining the decision-making task, dividing labor between humans and machines, interacting and collaborating between humans and machines, and implementing decision-making choices. Natural language processing, multimodal fusion, machine learning, and deep learning are the key technologies for humanmachine collaboration. Human-machine collaborative decisionmaking can be applied in fields such as business decision-making, intelligent production, intelligent driving, and medical diagnosis. In the future, human-machine collaboration will develop in directions such as large model experts, embodied intelligent agents, and braincomputer interface interactions.},
 author = {Liu, Zeshuang and Peng, Yan},
 booktitle = {2025 2nd International Conference on Artificial Intelligence and Digital Technology (ICAIDT)},
 doi = {10.1109/ICAIDT66272.2025.00027},
 title = {Human-Machine Interactive Collaborative Decision-Making Based on Large Model Technology: Application Scenarios and Future Developments},
 year = {2025}
}

@inproceedings{11146078,
 abstract = {Creative design has made significant progress with technological changes, and artificial intelligence tools, especially models based on Convolutional Neural Networks (CNN), are gradually becoming designers},
 author = {Li, Yitong},
 booktitle = {2024 International Conference on Internet of Things, Robotics and Distributed Computing (ICIRDC)},
 doi = {10.1109/ICIRDC65564.2024.00056},
 title = {The Application of Artificial Intelligence Tools Based on Convolutional Neural Networks in Creative Design},
 year = {2024}
}

@inproceedings{11147256,
 abstract = {Today, students face complex problems that require critical thinking skills, closely aligned with computational thinking concepts. It is increasingly important for students to shift from being passive consumers of content to actively creating content, using technology to address challenges. This shift has driven efforts to integrate computational thinking as a fundamental skill in K-12 educational programs. It is believed by incorporating computational thinking in the curriculum students will be better equipped to take on the challenges of the digital age. This belief is attributed to the fact that computational thinking includes a variety of problem-solving abilities like breaking down tasks, recognizing patterns, simplifying concepts, and thinking algorithmically. This study employs a systematic thematic synthesis approach, combining diverse research methodologies and using thematic analysis to explore effective strategies for assessing computational thinking in non-programming contexts. Literature advocates embedding computational thinking in K-12 education not only to prepare students for STEM careers but also to equip them with essential skills for problem-solving and decision-making in everyday life. The results indicate using assessments that combine short practical tasks along with multiple-choice and short-answer questions, offer an effective way to measure computational thinking skills, ensuring comprehensive evaluation and meaningful learning outcomes.},
 author = {Lynch, Jenice and Stokke, Thomas and Adgerson, Amber and Grant, Emanuel},
 booktitle = {2025 IEEE Integrated STEM Education Conference (ISEC)},
 doi = {10.1109/ISEC64801.2025.11147256},
 title = {Enhancing A Holistic Approach Towards Computational Thinking Assessment in Education},
 year = {2025}
}

@inproceedings{11147338,
 abstract = {This paper examines how computational thinking and engineering design can be integrated into STEM (science, technology, engineering, and math) education through a hands-on lesson focused on logistics and its societal impact. In this activity, middle school students program Ozobots (mini-robots used to teach coding) to model delivery routes for Amazon.com packages, a task that mirrors real-world challenges. The students combine coding skills with real-world problem-solving, gaining practical experiences that will serve them well in the future. Students will focus on the logistics when they create and refine delivery paths using block-based coding while tackling challenges like traffic patterns, delivery priorities, and route efficiency. The lesson encourages creativity, logical thinking, and collaboration while introducing key concepts in robotics, logistics, and computational thinking. Additionally, students critically analyze the societal implications of logistics systems by comparing synthetic and natural materials, with a focus on sustainability and ethical considerations. Societal influences further connect to the impacts of computing standards in a way that relates how individuals and communities affect computing by how they behave, interact culturally, and engage socially. This approach enhances STEM literacy and cultivates a deeper understanding of the interplay between technology, logistics, and societal needs. It is also an interdisciplinary lesson addressing science, computer science, engineering, and mathematics standards. By integrating science, technology, engineering, and mathematics, students can see the real-world connections between these subjects, encouraging a deeper understanding and preparing them for the complex challenges of the future. These lessons enhance their academic growth and inspire creativity and collaboration, essential skills for success in the rapidly evolving world.},
 author = {Cannella, Alaina and Douglass, Esther and Herbert, Katherine G. and Anu, Vaibhav and Hagiwara, Sumi and Marlowe, Thomas and Robila, Stefan and Goldstein, Rebecca},
 booktitle = {2025 IEEE Integrated STEM Education Conference (ISEC)},
 doi = {10.1109/ISEC64801.2025.11147338},
 title = {Integrating Computational Thinking and Engineering Design in STEM: Exploring Logistics and Societal Impact},
 year = {2025}
}

