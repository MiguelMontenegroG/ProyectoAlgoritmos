@inproceedings{10895574,
 abstract = {This paper introduces an AI-driven fashion design system that uses Generative Adversarial Networks (GANs) and real-time fabric simulations to create optimized sportswear, particularly for gymnastics. By simulating fabric properties such as stretchability, elasticity, and breathability, the system ensures garments provide comfort and support for dynamic movements. It also features an AI-powered Sustainability Score that offers instant feedback on the environmental impact of fabric and production choices, promoting eco-conscious decisions. Additionally, AI-generated fabric maintenance guidelines help extend the lifespan of garments by tailoring care instructions to specific materials. This model combines sustainability and performance optimization, offering a framework for integrating AI into fashion design.},
 author = {P, Dhanasree L and K, Chithra and A, Adlin Layola J and P, Deepthi and M, Bhuvaneshwari},
 booktitle = {2024 International Conference on Emerging Research in Computational Science (ICERCS)},
 doi = {10.1109/ICERCS63125.2024.10895574},
 title = {Revolutionizing Fashion: An AI-Driven Approach to Sustainable Design},
