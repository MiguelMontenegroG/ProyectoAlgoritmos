@inproceedings{9800954,
 abstract = {The purpose of the study is to identify and justify the main directions for improving the professional training of mathematics students in the context of informatization and digitalization. The article also considers examples of applying the obtained theoretical conclusions in the study of probability theory. The proposed lab provides an example of the application of information and communication technologies (ICT) for modeling and visualization to help students understand the basic concepts of probability theory. The authors conclude that there is a need for a consistent, expedient combination of traditional educational technologies and ICT in the educational process.},
 author = {Kuznetsova, Elena and Fomina, Tatiana and Karlova, Margarita and Zhbanova, Natalia},
