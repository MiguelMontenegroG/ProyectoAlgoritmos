 abstract = {In 1985, Diener and his colleagues presented an objective evaluation of life satisfaction using a questionnaire composed of five items: ideal, condition, satisfaction, acquisition, and change. Since Diener, many scholars have proposed a new measure of life satisfaction and have conducted various studies. Future life satisfaction so far, which is based on a 5-point Likert scale and the questioner},
 author = {Sung, Somin and Cha, Saem Gyeol and Choi, Seung Hoe},
