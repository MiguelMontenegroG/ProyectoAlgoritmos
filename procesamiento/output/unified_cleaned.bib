 abstract = {Trends of the school course in computer science are analyzed in comparison with the prognostic views of its founder, the academician A.P. Ershov. It is shown that there are core settings behind the external multidirectionality of target settings in different time periods during the course},
 author = {Gein, Aleksandr},
 booktitle = {2017 Fourth International Conference on Computer Technology in Russia and in the Former Soviet Union (SORUCOM)},
