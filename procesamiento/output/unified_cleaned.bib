@inproceedings{10999119,
 abstract = {Agile methodologies are gaining ground not only in the IT sector but also in other industries. The importance of collaboration skills in the modern workplace has increased exponentially with the rise of agile working.. At the heart of the agile approach are flexibility, adaptability and teamwork, all of which are closely linked to effective collaboration. Rather than traditional hierarchical structures, agile organisations are built around smaller, autonomous teams where shared goals and responsibilities prevail. This environment requires employees to be able to communicate effectively, share information and work together to achieve a common goal. Successful collaboration not only enables projects to be completed faster and more efficiently, but also fosters innovation and creativity, which is a key factor for competitive advantage in the 21st century. In an agile workplace, team members need to be able to adapt quickly to changing circumstances, receive and give feedback constructively and actively participate in joint problem-solving. In our research, we investigate the collaboration skills that are perceived as most important by employees in companies using agile methodologies, based on the results of a primary survey. The study was based on a survey of 274 respondents, and we were interested in how important they consider the different skills and competences required for collaboration to be. The aim of our survey is to identify the most important areas for improvement, pointing organisations in the right direction both in building more effective organisations and in developing their staff.},
 author = {Sterczl, Gábor and Csiszárik-Kocsir, Ágnes},
 booktitle = {2025 IEEE 12th International Conference on Computational Cybernetics and Cyber-Medical Systems (ICCC)},
 doi = {10.1109/ICCC64928.2025.10999119},
 title = {The Importance of Collaboration Skills in Agile Workplaces},
 year = {2025}
}

@inproceedings{11010717,
 abstract = {Urban areas are in the face of persistent traffic congestion, which disrupts city running and hinders sustainability attempts. There is no doubt that accurate traffic paragon prediction is crucial for effective intelligent transportation systems to some extent. However, current methods are beset by two main challenges: inadequate modeling of intricate traffic dynamics and limited hardware capabilities at the edge.This study introduces MSTF-Net, a novel framework that integrates diverse neural network architectures using adaptive fusion techniques. We propose a two-layer filtering approach after thinking thoroughly: one layer leverages static geographic boundaries, while the other adapts to dynamic traffic conditions. In terms of the practical meaning, this method deeply enhances computational efficiency while preserving prediction accuracy. For the innovative part, we developed a variable-precision FPGA implementation for the BiLSTM component, expeditiously balancing computational accuracy and energy efficiency for real-world applications. Evaluations on the PEMSD4 dataset demonstrate that MSTF-Net outperforms existing approaches, achieving an RMSE of 28.2 for short-term predictions (15 minutes) and maintaining robust performance over longer times. Compared to traditional fixedprecision methods, the reconfiguration strategy for the BiLSTM module reduces energy consumption by 2.8 times. This optimized implementation achieves 5.6 times higher power efficiency, in contrast to equivalent GPU-based execution. This paper presents a comprehensive hardware-software co-design methodology, offering practical insights for developing efficient traffic systems, making it less affected by the constrained traffic resources.},
 author = {Li, Jiayi and Gong, Yuting and Liu, Yao and Deng, Wenjie},
 booktitle = {2025 4th International Symposium on Computer Applications and Information Technology (ISCAIT)},
 doi = {10.1109/ISCAIT64916.2025.11010717},
 title = {Multi-Modal Traffic Flow Prediction with Dynamic FPGA Reconfiguration for Efficient Edge Computing},
 year = {2025}
}

