@inproceedings{11113627,
 abstract = {As the core quality of students in the 21st century, higher-order thinking has become an important goal of educational reform. As an emerging education mode, maker education provides an ideal practical scenario for the cultivation of higher-order thinking. This study explores the effect of peer-to-peer assessment activities on the development of higher-order thinking through a quasi-experimental design. Based on the core concepts of maker education, this study focuses on three aspects: creative, computational and reflective thinking. The results show that the experimental group improved significantly more than the control group in each thinking dimension.},
 author = {Wu, Yue and Hu, Qihui},
 booktitle = {2025 International Symposium on Educational Technology (ISET)},
