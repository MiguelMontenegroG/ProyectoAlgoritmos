@inproceedings{11027819,
 abstract = {Human-AI Collaboration has become increasingly common, with humans typically taking on the role of making final modification decisions in collaborative efforts. Due to the black-box nature of the AI generation process, it is often difficult for humans to understand the AI’s intentions in writing, specifically regarding what the AI considers to be strengths that should not be modified and where there is room for improvement. We propose a collaborative framework based on AI self-assessment, called PTCL-SE. To validate the effectiveness of AI self-assessment in collaborative contexts, we developed an online writing tool called SE-Etherpad and designed a collaborative experiment (N=25) to investigate the differences in collaborative outcomes with and without AI self-assessment. Following the experiment, we conducted a survey along with text evaluation and unicode. The results indicate that PTCL-SE enhances text quality, effectively alters revision strategies, and is generally popular.},
 author = {Cao, Xun},
 booktitle = {2024 17th International Symposium on Computational Intelligence and Design (ISCID)},
