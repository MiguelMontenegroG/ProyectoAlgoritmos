}

@inproceedings{11129413,
 abstract = {This study aimed to (1) develop a digital gamebased learning model to enhance computational problemsolving skills in the Engineering Materials subject, and (2) examine the effects of implementing the model on students’ problem-solving performance. The sample comprised 41 first-year undergraduate students enrolled in the Bachelor of Education program in Electrical Engineering at King Mongkut’s University of Technology North Bangkok. The research tools consisted of (1) educational game applications, (2) computational skill tests, and (3) a satisfaction questionnaire. The results showed: (1) the model achieved an E1/E2 efficiency of 87/85, meeting the expected benchmark, (2) students’ post-intervention scores in computational problem-solving were significantly higher than their pre-intervention scores ($\mathbf{p},
