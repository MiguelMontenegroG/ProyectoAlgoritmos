 year = {2024}
}

@inproceedings{10707828,
 abstract = {STEAM education and Computational Thinking emphasize interdisciplinary, integrated learning. However, the STEAM learning scheme designed for the ART domain is limited. Therefore, this study develops an Interactive Picture Storybook Scheme for Lower-Grade Elementary Students to Learn Music and Computational Thinking. It combines picture storybooks to provide narrative contexts and knowledge learning with interactive digital content that offers interactive scenarios and hands-on activities, enhancing learning motivation and effectiveness.},
 author = {Su, Jun-Ming and Lin, Yu-En and Hsu, Wei-Fang and Wu, Ting-Ti},
 booktitle = {2024 16th IIAI International Congress on Advanced Applied Informatics (IIAI-AAI)},
 doi = {10.1109/IIAI-AAI63651.2024.00148},
 title = {An Interactive Picture Storybook Scheme for Lower-Grade Elementary Students to Learn Music and Computational Thinking},
 year = {2024}
}

@inproceedings{10712570,
 abstract = {The article explores the concept of knowledge and knowledge models concerning their application in evaluating the assimilation of methodological knowledge. It analyzes the benefits of employing test formats for assessment in the learning process and their influence on the enhancement of students’ critical thinking and analytical skills. The potential for automating the generation of test sets using computer technologies is investigated. A framework for formalizing problem scenarios based on ontology models is proposed, aiming to organize and generalize knowledge in this domain. A method for generating problem scenarios to assess methodological knowledge is developed, leveraging computer algorithms and artificial intelligence. This approach facilitates the creation of high-quality test sets with diverse tasks and scenarios, thereby enhancing the effectiveness of evaluating students’ knowledge. Through experimental research, a system of variable parameters is identified, enabling the customization of test sets to different student groups and educational contexts. The results obtained validate the efficacy of the proposed method and underscore its significance in enhancing the assessment and teaching of methodological knowledge in educational settings.},
 author = {Melnyk, Andriy and Hirnyak, Andriy and Hirnyak, Halyna and Mudrak, Mariya and Hevko, Ihor and Popovych, Yuriy},
 booktitle = {2024 14th International Conference on Advanced Computer Information Technologies (ACIT)},
 doi = {10.1109/ACIT62333.2024.10712570},
 title = {Automatic Generation of Problem Scenarios for Assessment of Methodological Knowledge},
 year = {2024}
}

@inproceedings{10714517,
 abstract = {This paper introduces FlowPilot, a novel flow-based visual programming language designed to enhance Computational Thinking (CT) education. FlowPilot leverages generative AI to create a dynamic, browser-based environment where users can construct programs using natural language descriptions. By integrating AI-driven block generation with a flow-based visual interface, FlowPilot supports key CT pillars such as abstraction and decomposition. This approach offers a unique platform for learners to explore programming concepts at various levels of complexity, fostering a deeper understanding of computational processes.},
 author = {Turchi, Tommaso},
 booktitle = {2024 IEEE Symposium on Visual Languages and Human-Centric Computing (VL/HCC)},
 doi = {10.1109/VL/HCC60511.2024.00046},
 title = {FlowPilot: A Generative AI-Driven Visual Language for Computational Thinking Education},
 year = {2024}
}

@inproceedings{10714578,
 abstract = {While block-based programming has successfully eliminated critical syntactic barriers to programming, it remains unclear how effectively it aids in overcoming semantic, logical, and pragmatic programming challenges that hinder computational thinking. These challenges are likely to far outweigh the syntactic ones. With the goal of creating a highly accessible programming tool for young students using mobile devices, we explored the concept of pragmatic prebugging to begin addressing these challenges. By pragmatic prebugging, we refer to proactive debugging tools designed to prevent logical errors. This article introduces RULER.game as a Computational Thinking Tool with built-in pragmatic prebugging, enabling novice programmers to create games through a paradigm we call proxy-based programming. A small study exploring error rates found statistically significant performance improvements of proxy-based programming compared to block-based programming.},
 author = {Repenning, Alexander and Basawapatna, Ashok},
 booktitle = {2024 IEEE Symposium on Visual Languages and Human-Centric Computing (VL/HCC)},
 doi = {10.1109/VL/HCC60511.2024.00037},
 title = {RULER: Prebugging with Proxy-Based Programming},
 year = {2024}
}

