@inproceedings{11108534,
 abstract = {Large language models(LLMs) demonstrate exceptional performance in understanding and generating language but face challenges in tackling complex decision-making tasks. Researchers have introduced LLM-based agent workflows to address real-world complex problems. However, these workflows rely heavily on manual design, which is not only time-intensive but also requires significant expertise and lacks adaptability to diverse scenarios and dynamic environments. This paper introduces a novel approach for automatic generation of agentic workflows powered by LLMs, leveraging a meta-cognitive based Chain-of-Thought (MC-CoT) guidance integrated with Monte Carlo Tree Search (MCTS). By combining MC-CoT for structured reasoning with MCTS for optimization, the framework dynamically adjusts workflows, delivering superior performance across benchmarks such as GSM8K and MATH. Experimental results demonstrate the system},
 author = {Zhou, Kaiwen and Du, Wenli and Tian, Xufei},
 booktitle = {2025 7th International Conference on Natural Language Processing (ICNLP)},
 doi = {10.1109/ICNLP65360.2025.11108534},
 title = {Agentic Workflows Generation Based on Meta-Cognitive Chain-of-Thought Guided Monte Carlo Tree Search},
 year = {2025}
}

@inproceedings{11108869,
 abstract = {In the era of digital transformation, Computational Thinking (CT) has emerged as a core 21st-century competency, providing a systematic approach to problem-solving. Programming education is widely recognized as a key avenue for cultivating CT skills. However, current online programming platforms often fall short, facing challenges such as insufficient CT development, limited interactivity, and suboptimal learning outcomes. To address these issues, this study proposes the integration of Artificial Intelligence into instructional design to enhance the effectiveness of online programming education. Guided by the Human-AI Co-Creation Model, the research investigates the collaborative roles of educators and AI, and outlines a design framework for developing instructional content. Case studies demonstrate that this model significantly improves learners},
 author = {Zhuang, Yaojie and Ji, Yi and Huang, Zhaowen and Shang, Yujiao and Li, Xiuhong and Su, Xiaomin},
 booktitle = {2025 IEEE 5th International Conference on Software Engineering and Artificial Intelligence (SEAI)},
