 abstract = {The paper analyzes the construction condition and influencing factors of the concrete dam, and then constructs a block election model, which uses object-oriented simulation methods to model the construction of concrete dams. On the foundation of the preliminary research, a simulation and decision support system for concrete dam construction based on object-oriented is established to support decision-making.},
 author = {Gao, Hui and Li, Hong and Xu, Yingcheng and Zeng, Junwei},
