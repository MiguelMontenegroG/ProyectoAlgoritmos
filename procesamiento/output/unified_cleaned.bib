@article{9744486,
 abstract = {This article illustrates that the task design and problem selection are of characteristic influence to evoke sense-reason-act programming (SRA) among primary school pupils when programming robots. Research shows that the task design influences the development of computational thinking (CT). The literature provides evidence that the context, the problem space, and the representation of the problem to apply SRA-programming require the programming task to be embedded in a dynamic context in which a programmable robot must use sensory information to anticipate changes in the environment. In order to ascertain whether the problem space and the task design influence the evocation of SRA-thinking, this article uses a research design comparing the differences between two programming conditions (static/dynamic). In these conditions, pupils use Lego EV-3 robots and Mindstorms software to solve programming problems. As a post-measurement, a Lego challenge is applied. In this article, it is shown that the integration of a dynamic task design to solve a programming problem is essential for a deeper understanding of CT skills. Furthermore, when pupils can immediately test the consequences of their program in a dynamic environment and, thus, the learning environment provides an appropriate problem, they gain a deeper understanding of the added value of sensors and will be better able to reason about complex problems. It is found that programming in a dynamic problem environment almost naturally evokes SRA-thinking, as opposed to programming in a static environment. The influence of SRA-programming as demonstrated identifies characteristics of CT.},
 author = {Fanchamps, Nardie and Slangen, Lou and Specht, Marcus and Hennissen, Paul},
 doi = {10.1109/TLT.2022.3162895},
 journal = {IEEE Transactions on Learning Technologies},
 title = {The Effect on Computational Thinking Using SRA-Programming: Anticipating Changes in a Dynamic Problem Environment},
 year = {2022}
}

@inproceedings{9747320,
 abstract = {Speech summarization is typically performed by using a cascade of speech recognition and text summarization models. End-to-end modeling of speech summarization models is challenging due to memory and compute constraints arising from long input audio sequences. Recent work in document summarization has inspired methods to reduce the complexity of self-attentions, which enables transformer models to handle long sequences. In this work, we introduce a single model optimized end-to-end for speech summarization. We apply the restricted self-attention technique from text-based models to speech models to address the memory and compute constraints. We demonstrate that the proposed model learns to directly summarize speech for the How-2 corpus of instructional videos. The proposed end-to-end model outperforms the previously proposed cascaded model by 3 points absolute on ROUGE. Further, we consider the spoken language understanding task of predicting concepts from speech inputs and show that the proposed end-to-end model outperforms the cascade model by 4 points absolute F-1.},
 author = {Sharma, Roshan and Palaskar, Shruti and Black, Alan W and Metze, Florian},
 booktitle = {ICASSP 2022 - 2022 IEEE International Conference on Acoustics, Speech and Signal Processing (ICASSP)},
 doi = {10.1109/ICASSP43922.2022.9747320},
 title = {End-to-End Speech Summarization Using Restricted Self-Attention},
 year = {2022}
}

@inproceedings{9750655,
 abstract = {Based on investigations of the current situation of deep learning in computer basic courses in a certain medical university, the study aims to analyze the reasons of shallow learning in computer basic courses and find out the deep learning strategies. The questionnaire is designed from four dimensions: learning motivation, teaching methods, learning methods and learning effects. The results of the questionnaire show that the reasons for the lack of deep learning of computer basic courses are as follows: lack of novel content, too large class size, uneven basic level of students, only shallow learning for examinations and single evaluation method. The conclusion is introducing the teaching concept of deep learning into computer basic courses can change the teaching paradigm for the purpose of knowledge transfer and enable students to deeply participate in and effectively experience learning.},
 author = {Liu, Guihua and Wang, Gan and Wang, Jincai and Peng, Yuzhong},
 booktitle = {2021 11th International Conference on Information Technology in Medicine and Education (ITME)},
 doi = {10.1109/ITME53901.2021.00125},
 title = {An Analysis of the Current Situation of Computer Basic Courses in a Medical University and Its Countermeasures},
 year = {2021}
}

@inproceedings{9751858,
 abstract = {social media in Education has worked like a revolution in changing the learning system. Learning according to the convenience such as place and time of the students has pulled more interest to educate more through social media. In this study, we had investigated the perspective of students from different educational levels towards social media learning. A total no. of 130 students participated in our online survey. Convenient sampling was used for the study. According to the results and findings of the study learning through social media has both positive and negative impacts. The majority of respondents spend 1–3 hours a day on average. Additionally, we found that Youtube is the most used application by students for social media learning among Facebook, Instagram, Telegram, Whatsapp, and YouTube. Although learning through social media makes a great contribution for learners in their area of interest. However, it has also affected the physical and mental health of the students. Due to social media learning, socialization among people has been reduced.},
