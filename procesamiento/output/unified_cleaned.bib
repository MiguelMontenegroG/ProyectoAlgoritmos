@inproceedings{11137606,
 abstract = {This study aims to compare the psychometric properties of mathematics problem-solving assessment tools generated by Generative AI (Chat GPT) and those developed by humans. The participants included 130 students from 4 schools, selected through stratified sampling based on school size, out of 84 schools under the secondary educational service area office Khon Kaen. The mathematical problem-solving assessments (AI-generated and human-developed) were administered via Google Forms. The study employed Construct Modeling and Unidimensional Rasch Analysis to evaluate validity evidence, reliability evidence, quality of item fit, and the Turing test. The findings indicate that both tools are similar in content validity, but the AI-generated assessment demonstrates slightly higher accuracy in several aspects. Nevertheless, the human-developed assessment offers a better balance between the test difficulty and the examinees},
 author = {Phansrisuk, Panjit and Junpeng, Putcharee and Intharah, Thanapong},
 booktitle = {2025 International Technical Conference on Circuits/Systems, Computers, and Communications (ITC-CSCC)},
 doi = {10.1109/ITC-CSCC66376.2025.11137606},
 title = {Comparison of the Psychometric Properties of Mathematical Problem-Solving Assessment Tools for Grade 7 Students: AI-Generated vs. Human-Developed},
 year = {2025}
}

@inproceedings{11137621,
 abstract = {This study compares an AI-generated English Reading Comprehension test (Claude) with a human-created version on the topic of places. The research aims to evaluate the quality of AI-generated assessments with human design. Data from 128 ninth-grade students were analyzed using the Rasch model. Findings indicate that the AI-generated test demonstrated comparable content validity (CVI ratings consistently 3–4) to the human-made version while exhibiting superior reliability within acceptable psychometric limits. In a Turing Test, two English proficiency experts were more successful in identifying AI-generated items compared to experts from other fields. These results support the potential of AI in developing valid and extensible educational assessments.},
 author = {Khamrassamee, Pimvaree and Junpeng, Putcharee and Intharah, Thanapong},
 booktitle = {2025 International Technical Conference on Circuits/Systems, Computers, and Communications (ITC-CSCC)},
 doi = {10.1109/ITC-CSCC66376.2025.11137621},
 title = {The Effectiveness of Using Large Language Model to Generate English Reading Comprehension Diagnostic Test: Generative AI and Human-Made comparison},
 year = {2025}
}

@inproceedings{11139209,
 abstract = {Computational Thinking (CT) promotes the systematic resolution of complex interdisciplinary problems through abstraction, decomposition, pattern recognition, and modularization. As an extension of scientific reasoning, CT enhances problem modeling, resource allocation, and decision-making efficiency across various domains. This paper examines the fundamental principles of CT and demonstrates its practical applications in Object-Oriented Programming (OOP) and the E-CARGO model. Furthermore, it presents a case study on immigration policy making, illustrating how abstraction and classification improve problem modeling, while divide-and-conquer techniques enable computation and task assignment. The experimental results validate the superiority of CT-driven methods over traditional approaches in speed, accuracy, and cost-effectiveness. By integrating human reasoning with computational methodologies, CT provides a fundamental methodology for addressing large-scale optimization challenges, advancing both scientific research and applied problem-solving.},
 author = {Gong, Ke and Zhu, Haibin and Yang, Tianshuo},
 booktitle = {2025 1st International Symposium on E-CARGO and Applications (E-CARGO)},
 doi = {10.1109/E-CARGO65996.2025.11139209},
 title = {Computational Thinking, Object-Oriented Programming to Role-Based Collaboration},
 year = {2025}
}

@inproceedings{11139353,
 abstract = {Social media platforms such as YouTube, TikTok, and Reddit generate vast amounts of user-generated content, making sentiment analysis essential for understanding public opinion. Traditional sentiment analysis methods often struggle with sarcasm, informal language, and evolving trends, leading to the rise of Large Language Models (LLMs) as more effective alternatives. However, the choice between cloud-based LLMs (e.g., ChatGPT-4o, Gemini 2.0 Flash) and local LLMs (e.g., Meta LLaMA 3.2, DeepSeek-R1 7b) remains an open question, with each approach offering distinct advantages and limitations.This study compares the performance of online and local LLMs in sentiment analysis, focusing on discussions about AI-generated art and cancel culture. Through structured sentiment classification and comparative evaluation, we assess each model’s accuracy, efficiency, and interpretative depth. Our findings indicate that cloud-based models excel in contextual comprehension, while local models provide enhanced privacy and structured data processing. The results highlight the trade-offs between computational power, data security, and adaptability, reinforcing the potential of hybrid approaches that integrate both cloud and local LLM capabilities for more effective sentiment analysis.The goal of this research is to provide a clear evaluation of different LLMs in sentiment analysis, offering practical insights into their strengths and weaknesses. By comparing models from multiple developers, we aim to determine the most effective deployment strategies for real-world applications, ensuring accurate and scalable sentiment evaluation in diverse online discussions.},
 author = {Suindikovna, Riza Akhitova and David Gennadievych, Novikov and Sadyrbekkyzy, Akbergen Dinara},
 booktitle = {2025 IEEE 5th International Conference on Smart Information Systems and Technologies (SIST)},
 doi = {10.1109/SIST61657.2025.11139353},
 title = {From Comments to Insights: Comparing Local and Online LLM Models in Social Media Sentiment Evaluation},
 year = {2025}
}

@inproceedings{11141205,
 abstract = {Our analysis introduces how visual metaphors are used as a design tool in hybrid product design and exploratory, speculative use of the Contrastive LanguageImage Pretraining (CLIP) model. As hybrid products bridge the gap between physical and digital, then the use of AI generated visuals as metaphors can entice a more seamless transition for the user. Using an experimental method, the research analyses the kinds of visual metaphors and their influence on user satisfaction, commitment, and the perceived ability of the product. The findings indicate CLIP-inspired metaphors are highly capable of establishing an emotional connection getting a score of 9.5 out of 10, while higher concept designs perform better with more literal, intuitively aligned metaphors. This paper has demonstrated the potential of this AI approach to optimize the design process, resulting in personalized and context-appropriate metaphors that enhance user interaction and satisfaction. The insights gathered from the research foster a better understanding of the potential of AI in hybrid product development and the users that can be developed from it.},
 author = {Mohammed, Sumayah Abd Al Whaab and Lelo, Ahmed Talib},
 booktitle = {2025 IEEE 4th International Conference on Computing and Machine Intelligence (ICMI)},
 doi = {10.1109/ICMI65310.2025.11141205},
 title = {The Role of Visual Metaphors in Hybrid Product Design Using AI},
 year = {2025}
}

@inproceedings{11141259,
 abstract = {Nature has long been an inspiration for the design of various industrial products, boasting optimized structures and functional efficiency. The aim in this study is to help apply the new generation technologies, to take a source of inspiration from nature for new generation industrial designs. Mathematical techniques like fractals are being used to model organic systems accurately to improve product efficiency, beauty, and sustainability. The study assesses biomimetic designs using three primary metrics: fractal similarity, structural efficiency, and human expert opinion. The findings show that biomimetic designs are superior both structurally and aesthetically than traditional methods indicating a viable realistic application in verticals inclusive of architecture, automotive, and consumer electronics. These findings shed light on the transformative role in industrial design, merging the harmony of nature and technology.},
 author = {Sadiq, Safa Jaafar and Manea, Anis Hatem},
 booktitle = {2025 IEEE 4th International Conference on Computing and Machine Intelligence (ICMI)},
 doi = {10.1109/ICMI65310.2025.11141259},
 title = {Employing Modern Technologies in Drawing Inspiration from Nature to Develop Form in Industrial Products},
 year = {2025}
}

@inproceedings{11141300,
 abstract = {This paper investigates the changing role of Artificial Intelligence in terms of directing theatre productions and responding to the developing new territory of digital performances. To mitigate these hurdles of narrative coherence, aggregate interpretation of visual metaphor, and audience engagement, this study introduces a computational framework that embeds artificial intelligence in script analysis, scene management, and performance design. The methodology sutured together script-visual alignment models, emotional tone extractors, and staging optimizers in their own methodology, then tested it all on a data set of contemporary digital theater productions. The outcomes demonstrate considerable advancements in scene transitions, as well as director satisfaction and an audience emotional response, underscoring the ability for AI to be a co-creative partner rather than a replacement. These insights lay a robust foundation for the intelligent augmentation of both live and virtual performances, representing a paradigm shift in theatrical production practices.},
 author = {Latif, Ali Saad and Ismael, Muthanna Fadel and Khazal, Khattab Khaldon},
 booktitle = {2025 IEEE 4th International Conference on Computing and Machine Intelligence (ICMI)},
 doi = {10.1109/ICMI65310.2025.11141300},
 title = {The Role of Ai in Theater Directing: New Approaches to Digital Performances},
 year = {2025}
