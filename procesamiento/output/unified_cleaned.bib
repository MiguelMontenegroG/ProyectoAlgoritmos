@inproceedings{10868044,
 abstract = {Although the research on AI-assisted learning rapidly accelerates, there is still a notable gap in investigation into AI-generated content (AIGC) tool use in interdisciplinary academic writing. The present study addresses this gap by investigating the preference of AIGC use by science master’s students in writing interdisciplinary research proposals, and relationship between the use of AIGC tools and their writing abilities was assessed. Through a mixed-methods approach, we collected two iterations of research proposals from 33 students across diverse disciplines, along with their reflections on ChatGPT use. Faculty from both substantiation-track and teaching-track backgrounds were invited to evaluate the proposals and provide individual feedback. Quantitative topic modeling was used to identify theme patterns and areas where AI feedback contributed to improvements.The results showed a strong correlation between AIGC tool usage and higher writing scores, with students reporting that AI significantly enhanced the clarity, coherence, and technical accuracy of their proposals. However, findings also underscore that AI tools are less capable of handling deeper reasoning of niche and the methodological development required for writing in interdisciplinary contexts. Therefore, a balanced approach is necessary, where AI assists in technical aspects and human guidance focuses on advanced reasoning and structuring ideas.},
 author = {Luo, Lan and Liu, Jun and Xing, Qingqing},
 booktitle = {2024 4th International Conference on Educational Technology (ICET)},
 doi = {10.1109/ICET62460.2024.10868044},
 title = {Integrating AI and Human Feedback to Optimize Interdisciplinary Proposal Writing in Science Graduate Students},
 year = {2024}
}

@inproceedings{10868477,
 abstract = {Computational thinking (CT) is a crucial 21st-century skill, and its integration into K–12 education underscores the need for effective assessment tools. Although there are existing tools to measure CT practices, improvements can be made in terms of their assessment formats and data analysis methods. To address this gap, this study develops Critters Puzzle (CP), a game-based assessment tool aimed at measuring primary school students’ CT practices, specifically abstraction skills. Based on the Evidence-Centered Game Design (ECGD) framework, 12 progressively challenging tasks were developed. To ensure the content validity of the tool, validation was put forward involving expert review and students’ cognitive interviews. Twenty-two experts were invited to review the appropriateness of the measurement and seven students (aged 6–10) were invited for interviews to evaluate the usability of the platform and its effectiveness as an assessment tool. Results indicated that the game is engaging, understandable, and appropriately challenging for students, and several modifications were made to the game based on the feedback from experts and interviewees. Future research will involve empirical studies to validate the game},
 author = {Luo, Qi and Zhang, Shuhan},
 booktitle = {2024 4th International Conference on Educational Technology (ICET)},
 doi = {10.1109/ICET62460.2024.10868477},
 title = {Developing a Game to Assess Abstraction Skills of Elementary Students: Design Principles and Validation Process},
 year = {2024}
}

