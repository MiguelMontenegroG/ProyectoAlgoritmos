 title = {A Practical Teaching Model of Software Engineering Courses for Artificial Intelligence Literacy Cultivation in Pre-Service Teachers},
 year = {2025}
}

@article{11126173,
 abstract = {High-power microwaves (HPMs) are short-pulse electromagnetic waves in the microwave spectrum, characterized by intense impact energy that can cause damage to both electronic devices and human organs. In recent years, with the rapid advancement of data-driven artificial intelligence paradigms, achieving intelligent protection against HPM has emerged as a critical research challenge. The key to enabling intelligent analysis of plasma-based HPM protection lies in accurately modeling the complex multiphysics interactions between HPM and plasma. Existing methods have made progress in predicting the response of plasma characteristic parameters (such as electron density and electric intensity) under HPM exposure and in imputing missing values. However, it primarily focuses on short-term variations of single-feature variables. In this work, we propose a novel time-series modeling approach that integrates multiperspective feature extraction with multihead attention-based information fusion, complemented by an autonomous thinking strategy (ATS). This framework enhances the precise capture of long-term multivariate time-series patterns, and our experiments demonstrate its effectiveness. Furthermore, this study specifically investigates the coupling effects between HPM and a cylindrical plasma array (CPA). By conducting case studies and comparing our method with three state-of-the-art time-series models, we validate the superior accuracy and generalizability of our proposed approach.},
 author = {Feng, Naixing and Dong, Chunzhi and Fu, Jianyuan and Zheng, Fanghua and Huang, Zhixiang},
 doi = {10.1109/TMTT.2025.3596862},
 journal = {IEEE Transactions on Microwave Theory and Techniques},
 title = {MVPTransformer: Long-Term Multivariate Time-Series Pattern for Plasma Array-Based Intelligent Protection Against HPM},
 year = {2025}
}

@inproceedings{11126322,
 abstract = {Cooperative embodied AI systems, where multiple agents collaborate to accomplish complex, long-horizon tasks, show significant promise for real-world applications. These systems integrate perception, cognition, and action through integrated task and motion planning (TAMP), leveraging the advanced reasoning and communication capabilities of large language models (LLMs). However, their efficiency is often hindered by challenges such as high computational latency and redundant communication, largely due to the reliance on LLMs for sequential planning decisions. In this paper, we aim to identify the inherent characteristics and optimization opportunities in cooperative embodied AI systems. We first present a cognitive-inspired modular framework encompassing perception, memory, communication, planning, and execution. We then conduct a detailed profiling analysis of two state-of-the-art cooperative embodied systems, revealing the significance of each module and identifying critical bottlenecks such as redundant message pre-generation and excessive LLM usage in decision-making processes. Based on these insights, we propose several model- and system-level optimizations, including a planning-first communication strategy, selective multi-agent communication, and planning-guided multi-step execution. Evaluated across long-horizon cooperative tasks, these optimizations reduce the frequency of LLM inference runs, achieving an average 3.93× speedup in end-to-end task execution. Finally, we discuss the challenges and potential directions for embodied AI computing, to enhance system flexibility, efficiency, and scalability.},
 author = {Wan, Zishen and Du, Yuhang and Ibrahim, Mohamed and Zhao, Yang and Krishna, Tushar and Raychowdhury, Arijit},
 booktitle = {2024 ACM/IEEE International Conference On Computer Aided Design (ICCAD)},
 doi = {10.1145/3676536.3698389},
 title = {Thinking and Moving: An Efficient Computing Approach for Integrated Task and Motion Planning in Cooperative Embodied AI Systems (Invited Paper)},
 year = {2024}
}

@inproceedings{11129259,
 abstract = {Computational Thinking (CT) plays a fundamental role within Science, Technology, Engineering, and Mathematics (STEM) education. However, research examining the association between students’ attitudes toward STEM learning and their CT skills remains limited, particularly among lower primary school students. To address this gap, the present study adapted and validated an existing STEM Learning Attitude Scale for use with lower primary students in mainland China. The adapted scale was then employed to assess students’ attitudes toward STEM learning and to examine the relationship between these attitudes and their CT performance. A total of 171 second-grade students participated in the study. The psychometric properties of the adapted scale were evaluated, and the associations between students’ STEM learning attitudes and CT performance were analyzed using correlational analysis and linear regression. The findings confirmed that the adapted scale is a reliable and valid instrument for assessing STEM attitudes among young learners. Furthermore, students’ STEM learning attitudes were positively associated with CT performance, with mathematics learning attitudes exhibiting the strongest relationship. These results underscore the importance of promoting positive STEM attitudes in early education, particularly toward mathematics, to support the foundational development of CT skills.},
 author = {Luo, Qi and Hu, Yanqing and Wen, Jiamei},
 booktitle = {2025 10th International STEM Education Conference (iSTEM-Ed)},
 doi = {10.1109/iSTEM-Ed65612.2025.11129259},
 title = {Exploring the Relationship Between STEM Attitudes and Computational Thinking in Young Children},
 year = {2025}
}

@inproceedings{11129395,
 abstract = {This study aimed to develop a STEAM-based instructional activity package to enhance learning in the Computer-Based Control Systems subject and support 21stcentury competencies. The package included knowledge sheets, pre- and post-tests, achievement assessments, instructional media, worksheets, and IoT kits. Instrument validation was conducted by five experts, with analyses of item difficulty, discrimination, and reliability. The sample comprised 30 thirdyear students in the Industrial Electrical Engineering (IEE) program at Nakhon Sawan Rajabhat University. Results showed a significant improvement in learning outcomes, with pre-test and post-test averages of 26.50 and 45.67, respectively ($\mathbf{p},
